@using Microsoft.AspNetCore.Mvc.Localization
@inject IViewLocalizer Localizer
@{
    ViewData["Title"] = Localizer["Client Specific Requirements"];
    ViewData["PageTitle"] = Localizer["Client Specific Requirements"];
}

<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="card">

                    <div class="card-body">
                        <div class="toolbar">
                            <!--        Here you can write extra buttons/actions for the toolbar              -->
                            <span class="btn btn-file btn-success" style="display:none;">
                                <span class="fileinput-new"><i class="fa fa-upload"></i> Upload as Excel/csv</span>
                                <input type="file" name="importexcelfile" id="importexcelfile" onchange="UploadFile();" />
                            </span>
                        </div>

                        <div class="material-datatables">
                            <table id="CSRTable" class="table table-striped table-no-bordered table-hover" cellspacing="0" width="100%" style="width:100%">

                                <thead>
                                    <tr>
                                        <th width="5%">@Localizer["Client Code"]</th>
                                        <th width="15%">@Localizer["Company"]</th>
                                        <th width="15%">@Localizer["Remediation Requirements"]</th>
                                        <th width="15%">@Localizer["Common Alt"]</th>
                                        <th width="10%">@Localizer["Metadata/Author(s)"]</th>
                                        <th width="5%">@Localizer["Unsecured (FINAL)"]</th>
                                        <th width="5%">@Localizer["Secured (FINAL-ua or FINAL-uae)"]</th>
                                        <th width="5%">@Localizer["PDF/A (FINAL-a)"]</th>
                                        <th width="5%">@Localizer["PAC Reports"]</th>
                                        <th width="20%">@Localizer["Notes"]</th>
                                    </tr>
                                </thead>
                                <tbody>
                                </tbody>


                            </table>
                        </div>
                    </div>
                    <!-- end content-->
                </div>
                <!--  end card  -->
            </div>
            <!-- end col-md-12 -->
        </div>
        <!-- end row -->
    </div>
</div>
@section scripts{


    <!-- BEGIN PAGE LEVEL PLUGINS -->
    <script src="~/assets/js/pages/CSR.js?v=@abledoc.Utility.CommonHelper.Version"></script>
    <!-- END PAGE LEVEL PLUGINS -->
}